<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.user.aifileorganizer</string>
    <key>ProgramArguments</key>
    <array>
        <string>/path/to/your/ai-organizer/venv/bin/python</string>
        <string>/path/to/your/ai-organizer/organizer.py</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/path/to/your/ai-organizer/organizer.log</string>
    <key>StandardErrorPath</key>
    <string>/path/to/your/ai-organizer/organizer.err</string>
</dict>
</plist>
